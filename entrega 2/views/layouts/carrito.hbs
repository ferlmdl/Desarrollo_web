<div class="container">
    <div class="row">
        {{#each productos}}
            <div class="col">
                <div class="card">
                  <div class="card-body">
                    <h5 class="card-title">{{this.id.name}}</h5>
                    <p class="card-text">Precio: {{this.id.precio}} - Cantidad: {{this.qty}}</p>
                  </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>